#!/bin/bash
cd ~/git/dxvk-tools || exit 1
rm -r win32 win64
git pull
# x86_64 games
WINEARCH=win64 DXVK=1 WINEPREFIX="$HOME/Games/deusex" ./installvulkan.sh
# x86 + x86_64 games
WINEARCH=win64 DXVK=1 WINEPREFIX="$HOME/Games/awesome-spirit" ./installvulkan.sh
WINEARCH=win32 DXVK=1 WINEPREFIX="$HOME/Games/awesome-spirit" ./installvulkan.sh
WINEARCH=win64 DXVK=1 WINEPREFIX="$HOME/Games/gog-galaxy" ./installvulkan.sh
WINEARCH=win32 DXVK=1 WINEPREFIX="$HOME/Games/gog-galaxy" ./installvulkan.sh
